	 <div>a = {a}</div>
  </set>
  <div>a = {a}</div>

  <unset key="a"/>
  <div>a = {a}</div>

  <set key="a" value="ABC"/>
  <div>a = {a}</div>
  <del key="a"/>
  <div>a = {a}</div>

  <div>key = {key}</div>
  <del key="key"/>

  <with key="dict">
