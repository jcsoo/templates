  <get key="dict.c.y"/> = YYY

  # IF

  TRUE = <if key="true"> TRUE <else/> FALSE</if>
  TRUE = <if key="false"> FALSE <else/> TRUE</if>

  <switch key="test">
	 <case value="no">NO</case>
	 <case value="ABC">YES</case>
