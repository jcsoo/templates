<page parent="_site.xml">
<chunk id="main">
<table class="browse">
  <tr>
	 <th width="150">Path</th>
	 <th>Title</th>
  </tr>
  <loop key="_records">
	 <tr href="page;id={path|u}/">
		<td>{path}</td>
		<td>{title}</td>
	 </tr>
  </loop>
</table>
<form method="POST">
  <div class="actions">
  </div>
</form>
</chunk>
</page>